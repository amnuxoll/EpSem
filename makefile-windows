OUTDIRS :=  dir $(OUTDIR)

print-% : ; @echo $* = $($*)
classpathify = $(subst $(eval) ,:,$(wildcard $1))

default: all 

all:
	if not exist out mkdir out
	@$(JC) $(JARGS) $(ALLSRC)

runexperiments: all
	echo call
	$(JVM) -cp $(call classpathify,$(OUTDIRS)) experiments.Runner
	echo call

runtests: all
	$(JVM) -cp $(call classpathify,$(OUTDIRS)) tests.EpSemTestRunner out/tests

runtestsnorebuild:
	$(JVM) -cp $(call classpathify,$(OUTDIRS)) tests.EpSemTestRunner out/tests

clean: 
	del $(OUTDIRS)\$(FILES)